{
  "manifest_version": 2,
  "name": "TulioNizer",
  "description": "Tulios.",
  "version": "1.1",
  "icons": { "128": "icon_128.png" },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["tulify.js"]
    }
  ]
}
